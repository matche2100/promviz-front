{
  "name": "promviz-front",
  "version": "0.3.0",
  "description": "promviz front",
  "author": "Yusuke Otsuka <mjhd.devlion@gmail.com>",
  "scripts": {
    "postinstall": "bash vizceral_build.sh",
    "vizceral-install": "npm install ./vizceral",
    "clean": "rimraf dist && mkdirp dist",
    "lint": "eslint src --ext .js --ext .jsx --fix",
    "copy:fonts": "cpx \"./node_modules/source-sans-pro/**/*\" ./dist/fonts",
    "copy:json": "cpx \"./src/*.json\" ./dist",
    "dev-server": "webpack-dev-server --display-error-details --host=0.0.0.0 --content-base dist/ --history-api-fallback",
    "dev": "npm-run-all clean --parallel copy:* dev-server",
    "validate": "npm ls",
    "dist": "npm run clean && npm run copy:fonts && npm run copy:json && webpack",
    "start": "npm run dist && node server.js"
  },
  "engines": {
    "node": "~8.2.0"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "debug": "^4.1.0",
    "documentation": "^8.1.2",
    "express": "^4.16.2",
    "flux": "^3.1.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "keymirror": "^0.1.1",
    "keypress.js": "2.1.0-1",
    "lodash": "^4.17.11",
    "macaddress": "^0.2.9",
    "mime": "^2.3.1",
    "numeral": "^1.5.3",
    "prop-types": "^15.6.2",
    "query-string": "^4.2.3",
    "randomatic": "^3.1.1",
    "react-addons-shallow-compare": "^15.3.2",
    "react-bootstrap": "^0.32.4",
    "react-checkbox-group": "^3.3.1",
    "react-virtualized": "^9.21.0",
    "safe-buffer": "^5.1.2",
    "socket.io-client": "^2.1.1",
    "source-sans-pro": "^2.0.10",
    "superagent": "^3.8.3",
    "tween.js": "^16.3.5",
    "webfontloader": "^1.6.26"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^3.2.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.1",
    "precommit-hook": "^3.0.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-highcharts": "^16.0.2",
    "react-overlays": "^0.8.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mjhd-devlion/promviz-front.git"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
